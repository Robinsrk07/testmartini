			</ul>

			
		</div>

		<!-- Modal Search -->
		

	<!-- Cart -->
	

	<!-- breadcrumb -->
	<div class="container">
		<div class="bread-crumb flex-w p-l-25 p-r-15 p-t-30 p-lr-0-lg">
			<a href="/user/user_logged" class="stext-109 cl8 hov-cl1 trans-04">
				Home
				<i class="fa fa-angle-right m-l-9 m-r-10" aria-hidden="true"></i>
			</a>

			<span class="stext-109 cl4">
			 Add addres
			</span>
		</div>
	</div>
		
	<a href="/user/addAdress1"><button type="button" class="btn btn-dark" style="margin-left: 1150px;">Add Address</button></a>

	<!-- Shoping Cart -->
	
				<!-- Left column (existing cart items) -->
                <style>
                    .breadcrumb-nav {
                        
                        align-items: center;
                    }
                    .breadcrumb-nav h6 {
                        margin-right: auto;
                    }
                    .breadcrumb-nav .btn {
                        margin-left: 700px;
                        margin-top: 10px;
                    }
                    .address-card {
                        background-color: #fff;
                        border: 1px solid #ddd;
                        border-radius: 10px;
                        padding: 20px;
                        margin-bottom: 20px;
                        position: relative;
                        transition: box-shadow 0.3s ease;
                    }
                    .address-card:hover {
                        box-shadow: 0 4px 8px rgba(0,0,0,0.1);
                    }
                    .address-field {
                        margin-bottom: 10px;
                    }
                    .field-name {
                        font-weight: bold;
                        color: #333;
                    }
                    .field-value {
                        color: #666;
                    }
                    .address-toggle {
                        position: absolute;
                        top: 10px;
                        right: 10px;
                    }
                    .address-toggle input[type="checkbox"] {
                        display: none;
                    }
                    .address-toggle label {
                        cursor: pointer;
                        padding: 5px 10px;
                        background-color: #f1f1f1;
                        border-radius: 15px;
                        transition: background-color 0.3s ease;
                    }
                    .address-toggle input[type="checkbox"]:checked + label {
                        background-color: #28a745;
                        color: white;
                    }
                    .address-toggle {
                        display: flex;
                        flex-direction: column; /* Stack items vertically */
                      }
    
                      /* Style for each child element within the address-toggle container */
                      .address-toggle input[type="checkbox"],
                      .address-toggle label,
                      .address-toggle button {
                        margin-bottom: 5px; /* Add margin to separate elements vertically */
                      }
                    
                </style>
               
           
               <form class="bg0 p-t-75 p-b-85">
                <div class="container">
                    <div class="row">
                        <!-- Left column (address cards) -->
                        <div class="col-lg-7 col-xl-7 m-lr-auto m-b-50">
                            <div class="container-fluid py-4">
                                <% address.forEach((addres, index) => { %>
                                    <div class="address-card">
                                        <div class="address-toggle">
                                            <a href="javascript:void(0);" onclick="redirect('<%= addres.id %>','<%= total %>','<%=price %>','<%=savings%>')">
                                                Deliver Here
                                            </a>
                                        </div><script>
                                            function redirect(addressId,total,price,savings) {
                                                window.location.href = `/user/getorderSummery?addressId=${addressId}&total=${total} &price=${price}&savings=${savings}`;
                                            }
                                        </script>
                                        <div class="address-field">
                                            <span class="field-name">Name:</span>
                                            <span class="field-value"><%= addres.name %></span>
                                        </div>
                                        <div class="address-field">
                            <span class="field-name">Phone Number:</span>
                            <span class="field-value"><%= addres.phoneNumber %></span>
                        </div>
                        <div class="address-field">
                            <span class="field-name">Email:</span>
                            <span class="field-value"><%= addres.email %></span>
                        </div>
                        <div class="address-field">
                            <span class="field-name">Address:</span>
                            <span class="field-value"><%= addres.address %></span>
                        </div>
                        <div class="address-field">
                            <span class="field-name">Locality:</span>
                            <span class="field-value"><%= addres.locality %></span>
                        </div>
                        <div class="address-field">
                            <span class="field-name">Pin Code:</span>
                            <span class="field-value"><%= addres.pinCode %></span>
                        </div>
                        <div class="address-field">
                            <span class="field-name">Additional Info:</span>
                            <span class="field-value"><%= addres.additionalInfo %></span>
                        </div>
                                    </div>
                                <% }); %>
                            </div>
                        </div>
            
            
                        <!-- Right column (cart totals) -->
						<div class="col-lg-5 col-xl-5 m-lr-auto m-b-50 mt-4">
							<div class="bor10 p-lr-40 p-t-30 p-b-40 m-l-63 m-r-40 m-lr-0-xl p-lr-15-sm">
								<h4 class="mtext-109 cl2 p-b-30">
									Cart Totals
								</h4>
						
								<div class="flex-w flex-t bor12 p-b-13 mb-2">
									<div class="size-208">
										<span class="stext-110 cl2">
											Price:
										</span>
									</div>
									<div class="size-209">
										<span class="mtext-110 cl2">
											RS. <%= price %>
										</span>
									</div>
									<div class="size-208 mt-4 mb-4">
										<span class="stext-110 cl2">
											Discount
										</span>
									</div>
									<div class="size-209 mt-4 mb-4">
										<span class="mtext-110 cl2">
											RS. <%= savings %>
										</span>
									</div>
									<div class="size-208 mt-4 mb-4">
										<span class="stext-110 cl2">
											Total:
										</span>
									</div>
									<div class="size-209 mt-4 mb-4">
										<span class="mtext-110 cl2">
											RS. <%= total %>
										</span>
									</div>
								</div>
							</div>
						</div>
                    </div>
                </div>
            </form>

   
			<footer class="bg3 p-t-75 p-b-32">
				<div class="container">
					<div class="row justify-content-between">
						<div class="col-sm-6 col-lg-3 p-b-50">
							<h4 class="stext-301 cl0 p-b-30">
								Categories
							</h4>
							<ul>
								<% category.forEach(function(category) { %>
									<li class="p-b-10">
										<a href="#" class="stext-107 cl7 hov-cl1 trans-04">
											<%= category.name %>
										</a>
									</li>
								<% }); %>
							</ul>
						</div>
				
						<div class="col-sm-6 col-lg-4 p-b-50 order-lg-3 p-2 ">
							<h4 class="stext-301 cl0 p-b-30">
								Newsletter
							</h4>
							<form>
								<div class="wrap-input1 w-full p-b-4">
									<input class="input1 bg-none plh1 stext-107 cl7" type="text" name="email" placeholder="pinkmartini@gmail.com">
									<div class="focus-input1 trans-04"></div>
								</div>
								<div class="p-t-18">
