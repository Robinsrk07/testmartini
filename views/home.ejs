<div class="row isotope-grid">
	<% products.forEach(product => { %>
	  <% if (product.variant && product.variant.images && product.variant.images.length > 0) { %>
		<div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item women">
		  <div class="block2">
			<div class="block2-pic hov-img0">
			  <div class="image-container" style="width: 100%; height: 0; padding-top: 100%; position: relative; overflow: hidden;">
				<img class="product-image" src="data:<%= product.variant.images[0].imagetype %>;base64,<%= product.variant.images[0].imagedata.toString('base64') %>" alt="Product Image" style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; object-fit: cover;">
			  </div>
			  <a href="#" class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04 js-show-modal1">Quick View</a>
			</div>
			<div class="block2-txt flex-w flex-t p-t-14">
			  <div class="block2-txt-child1 flex-col-l">
				<div class="product-info">
				  <a href="/user/product_detials/<%= product._id %>" class="product-title stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6"><%= product.title %></a>
				  <div class="product">
					<% if (product.variant.hasDiscount) { %>
					  <span class="product-price original-price"><s>RS.<%= product.variant.originalPrice %>/-</s></span>
					  <span class="product-price discounted-price">RS.<%= product.variant.discountedPrice %>/-</span>


					<% } else { %>
					  <span class="product-price">RS.<%= product.variant.originalPrice %>/-</span>
					<% } %>
					
					<div class="size-quantities">
					  <% if (product.variant.sizeQuantities && product.variant.sizeQuantities.length > 0) { %>
						<% const quantity = product.variant.sizeQuantities[0].quantity; %>
						<% if (quantity > 10) { %>
						  <span class="in-stock">In Stock</span><br>
						<% } else if (quantity > 0 && quantity <= 10) { %>
						  <span class="limited-stock">Only <%= quantity %> left</span><br>
						<% } else { %>
						  <span class="out-of-stock">Out of Stock</span><br>
						<% } %>
					  <% } else { %>
						<span>No sizes available</span>
					  <% } %>
					</div>
				  </div>
				  <!-- <div class="star-rating">
					<% for (let i = 1; i <= 5; i++) { %>
					  <span class="star <%= i <= product.rating ? '' : 'empty' %>">&#9733;</span>
					<% } %>
				  </div> -->
				</div>
