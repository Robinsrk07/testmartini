	function toggleWishlist(productId) {
    fetch(`/user/wishlist/${productId}`, {
        method: 'GET',
        headers: {
            'Content-Type': 'application/json',
        },
    })
    .then(response => response.json())
    .then(data => {
        console.log(data.message);
        // Update UI based on the response
        const wishlistButton = document.querySelector(`a[data-product-id="${productId}"]`);
        if (wishlistButton) {
            const addSpan = wishlistButton.querySelector('.wishlist-add');
            const removeSpan = wishlistButton.querySelector('.wishlist-remove');
            
            if (data.status === "added") {
                addSpan.style.display = 'none';
                removeSpan.style.display = 'inline';
                wishlistButton.classList.add('in-wishlist');
            } else if (data.status === "removed") {
                addSpan.style.display = 'inline';
                removeSpan.style.display = 'none';
                wishlistButton.classList.remove('in-wishlist');
            }
        }
    })
    .catch((error) => {
        console.error('Error:', error);
    });
}
